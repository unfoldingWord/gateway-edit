{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@context/*": ["src/context/*"],
      "@hooks/*": ["src/hooks/*"],
      "@styles/*": ["src/styles/*"],
      "@common/*": ["src/common/*"],
      "@utils/*": ["src/utils/*"],
      "translation-helps-rcl": ["../translation-helps-rcl/dist"],
      "translation-helps-rcl/*": ["../translation-helps-rcl/dist/*"],
    }
  },
  "include": [
    "./node_modules/cypress",
    "cypress/**/*.js"
  ]
}
